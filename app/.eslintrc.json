{
    "env": {
        "browser": true,
        "es2021": true,
        "react-native/react-native": true
    },
    "extends": ["plugin:react/recommended", "standard", "eslint-config-prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "react-native"],
    "rules": {
        "no-unused-vars": "warn",
        "react/prop-types": "off",
        "no-unneeded-ternary": "off",
        "camelcase": "off",
        "no-return-assign": "off"
    }
}
